{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "PRODUCTION_BACKEND_URL": "localhost:8000"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "PRODUCTION_BACKEND_URL": "nutricious4u-production.up.railway.app"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "PRODUCTION_BACKEND_URL": "nutricious4u-production.up.railway.app"
      }
    }
  },
  "submit": {
    "production": {}
  }
}