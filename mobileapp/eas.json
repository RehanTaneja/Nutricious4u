{
  "cli": {
    "version": ">= 5.9.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "simulator": true
      },
      "env": {
        "NODE_ENV": "development",
        "PRODUCTION_BACKEND_URL": "nutricious4u-production.up.railway.app"
      },
      "cache": {
        "disabled": false
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "NODE_ENV": "production",
        "PRODUCTION_BACKEND_URL": "nutricious4u-production.up.railway.app"
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "NODE_ENV": "production",
        "PRODUCTION_BACKEND_URL": "nutricious4u-production.up.railway.app"
      }
    }
  }
}